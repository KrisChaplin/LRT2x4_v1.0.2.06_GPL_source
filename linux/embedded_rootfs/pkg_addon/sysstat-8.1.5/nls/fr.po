#: common.c:57
#, c-format
msgid "sysstat version %s\n"
msgstr "sysstat version %s\n"

#: common.c:160 common.c:385 ioconf.c:459 mpstat.c:265 sa_common.c:601
#: sadc.c:476 sadc.c:485 sadc.c:530 sadc.c:620
#, c-format
msgid "Cannot open %s: %s\n"
msgstr "Impossible d'ouvrir %s: %s\n"

#: common.c:199
#, c-format
msgid "Cannot handle so many processors!\n"
msgstr "Trop de processeurs !\n"

#: iostat.c:74
#, c-format
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ]\n"
"Options are:\n"
"[ -c ] [ -d ] [ -N ] [ -n ] [ -k | -m ] [ -t ] [ -V ] [ -x ]\n"
"[ <device> [ ... ] | ALL ] [ -p [ <device> | ALL ] ]\n"
msgstr ""
"Utilisation: %s [ options... ] [ <intervalle> [ <itérations> ] ]\n"
"Options possibles:\n"
"[ -c ] [ -d ] [ -N ] [ -n ] [ -k | -m ] [ -t ] [ -V ] [ -x ]\n"
"[ <device> [ ... ] | ALL ] [ -p [ <device> | ALL ] ]\n"

#: iostat.c:1119
#, c-format
msgid "Time: %s\n"
msgstr "Heure: %s\n"

#: iostat.c:1435
#, c-format
msgid "-x and -p options are mutually exclusive\n"
msgstr "Les options -x et -p ne peuvent être utilisées ensemble\n"

#: mpstat.c:66
#, c-format
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ]\n"
"Options are:\n"
"[ -P { <cpu> | ALL } ] [ -V ]\n"
msgstr ""
"Utilisation: %s [ options... ] [ <intervalle> [ <itérations> ] ]\n"
"Options possibles:\n"
"[ -P { <cpu> | ALL } ] [ -V ]\n"

# sar.c:
#: mpstat.c:226 pidstat.c:1148 sar.c:740
msgid "Average:"
msgstr "Moyenne:"

#: mpstat.c:563 sar.c:157
#, c-format
msgid "Not that many processors!\n"
msgstr "Pas tant de processeurs !\n"

#: pidstat.c:72
#, c-format
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ]\n"
"Options are:\n"
"[ -C <comm> ] [ -d ] [ -I ] [ -r ] [ -t ] [ -u ] [ -V ] [ -w ]\n"
"[ -p { <pid> | SELF | ALL } ] [ -T { TASK | CHILD | ALL } ]\n"
msgstr ""
"Utilisation: %s [ options... ] [ <intervalle> [ <itérations> ] ]\n"
"Options possibles:\n"
"[ -C <comm> ] [ -d ] [ -I ] [ -r ] [ -t ] [ -u ] [ -V ] [ -w ]\n"
"[ -p { <pid> | SELF | ALL } ] [ -T { TASK | CHILD | ALL } ]\n"

#: pidstat.c:166 sar.c:1419
#, c-format
msgid "Requested activities not available\n"
msgstr "Statistiques demandées non disponibles\n"

#: sa_common.c:572
#, c-format
msgid "Error while reading system activity file: %s\n"
msgstr "Erreur lors de la lecture du fichier d'activité système: %s\n"

#: sa_common.c:581
#, c-format
msgid "End of system activity file unexpected\n"
msgstr "Fin du fichier d'activité système inattendue\n"

#: sa_common.c:608 sadc.c:549
#, c-format
msgid "Invalid system activity file: %s (%#x)\n"
msgstr "Fichier d'activité système invalide: %s (%#x)\n"

#: sa_common.c:632
#, c-format
msgid "Requested activities not available in file\n"
msgstr "Activitées demandées non enregistrées dans le fichier\n"

#: sadc.c:83
#, c-format
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ] [ <outfile> ]\n"
"Options are:\n"
"[ -C <comment> ] [ -d ] [ -F ] [ -I ] [ -V ]\n"
msgstr ""
"Utilisation: %s [ options... ] [ <intervalle> [ <itérations> ] ] [ <fichier> ]\n"
"Options possibles:\n"
"[ -C <commentaire> ] [ -d ] [ -F ] [ -I ] [ -V ]\n"

#: sadc.c:110
#, c-format
msgid "Cannot write data to system activity file: %s\n"
msgstr "Impossible d'écrire les données dans le fichier d'activité système: %s\n"

#: sadc.c:361
#, c-format
msgid "Cannot write system activity file header: %s\n"
msgstr "Impossible d'écrire l'en-tête du fichier d'activité système: %s\n"

#: sadc.c:568
#, c-format
msgid "Cannot append data to that file\n"
msgstr "Impossible d'ajouter des données à la fin de ce fichier\n"

#: sadf.c:78
#, c-format
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ] [ <datafile> ]\n"
"Options are:\n"
"[ -d | -D | -H | -p | -x ] [ -t ] [ -V ]\n"
"[ -P { <cpu> | ALL } ] [ -s [ <hh:mm:ss> ] ] [ -e [ <hh:mm:ss> ] ]\n"
"[ -- <sar_options...> ]\n"
msgstr ""
"Utilisation: %s [ options... ] [ <intervalle> [ <itérations> ] ] [ <fichier> ]\n"
"Options possibles:\n"
"[ -d | -D | -H | -p | -x ] [ -t ] [ -V ]\n"
"[ -P { <cpu> | ALL } ] [ -s [ <hh:mm:ss> ] ] [ -e [ <hh:mm:ss> ] ]\n"
"[ -- <sar_options...> ]\n"

#: sar.c:76
#, c-format
msgid ""
"Usage: %s [ options... ] [ <interval> [ <count> ] ]\n"
"Options are:\n"
"[ -A ] [ -b ] [ -B ] [ -c ] [ -C ] [ -d ] [ -i <interval> ] [ -p ] [ -q ]\n"
"[ -r ] [ -R ] [ -t ] [ -u ] [ -v ] [ -V ] [ -w ] [ -W ] [ -y ]\n"
"[ -I { <irq> | SUM | ALL | XALL } ] [ -P { <cpu> | ALL } ]\n"
"[ -n { DEV | EDEV | NFS | NFSD | SOCK | ALL } ]\n"
"[ -o [ <filename> ] | -f [ <filename> ] ]\n"
"[ -s [ <hh:mm:ss> ] ] [ -e [ <hh:mm:ss> ] ]\n"
msgstr ""
"Utilisation: %s [ options... ] [ <intervalle> [ <itérations> ] ]\n"
"Options possibles:\n"
"[ -A ] [ -b ] [ -B ] [ -c ] [ -C ] [ -d ] [ -i <intervalle> ] [ -p ] [ -q ]\n"
"[ -r ] [ -R ] [ -t ] [ -u ] [ -v ] [ -V ] [ -w ] [ -W ] [ -y ]\n"
"[ -I { <irq> | SUM | ALL | XALL } ] [ -P { <cpu> | ALL } ]\n"
"[ -n { DEV | EDEV | NFS | NFSD | SOCK | ALL } ]\n"
"[ -o [ <fichier> ] | -f [ <fichier> ] ]\n"
"[ -s [ <hh:mm:ss> ] ] [ -e [ <hh:mm:ss> ] ]\n"

#: sar.c:149
#, c-format
msgid "Not an SMP machine...\n"
msgstr "Pas une machine multiprocesseur...\n"

#: sar.c:1261
#, c-format
msgid "Invalid data format\n"
msgstr "Format de données invalide\n"

#: sar.c:1633
#, c-format
msgid "-f and -o options are mutually exclusive\n"
msgstr "Les options -f et -o ne peuvent être utilisées ensemble\n"

#: sar.c:1639
#, c-format
msgid "Not reading from a system activity file (use -f option)\n"
msgstr "Pas de fichier d'activité système en lecture (utilisez l'option -f)\n"
